{
  "name": "smoren/containers",
  "description": "Abstract data containers and structures for PHP",
  "keywords": ["abstract", "structures", "container", "collection", "list", "linked list", "map"],
  "license": "MIT",
  "authors": [
    {
      "name": "Smoren",
      "email": "ofigate@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "smoren/extended-exceptions": "^1.0",
    "smoren/helpers": "^0.1"
  },
  "require-dev": {
    "codeception/codeception": "^4.0",
    "codeception/module-asserts": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "Smoren\\Containers\\": "src",
      "Smoren\\Containers\\Tests\\Unit\\": "tests/unit"
    }
  },
  "config": {
    "fxp-asset": {
      "enabled": false
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
